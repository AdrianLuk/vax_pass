{
  "expo": {
    "name": "Vaccine Certificate",
    "slug": "vax_pass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/myvaccinepass.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you show your vaccine information."
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/myvaccinepass.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/myvaccinepass.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adrianluk.vax_pass"
    },
    "web": {
      "favicon": "./assets/images/myvaccinepass.png"
    }
  }
}
